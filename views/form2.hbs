<!DOCTYPE html>
<html lang="en">

    {{> header}}

<body >

<form method="POST" action="/picture" style="margin: 100px">
    <div class="form-group">
        <label for="picture_input">Picture Target:</label>
        <input type="text" class="form-control" name='picture_input' id="picture_input" required>
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>

    <p class="output" id="output">{{output}}</p>
</form>

    {{#each objects}}
        <img src='{{link}}' alt=""> <br>
    {{/each}}

</body>
</html>